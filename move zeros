class Solution {
    public void moveZeroes(int[] nums) {
        Stack<Integer> s = new Stack<Integer>();
        int n = nums.length;
        int noOfZeros=0;
        for(int num:nums){
            if(num == 0)
                noOfZeros++;        
            else
                s.push(num);
        }
        while(noOfZeros-->0)
            s.push(0);
        for(int i=n-1;i>=0;i--){
            nums[i] = s.pop();
        }
    }
}
